{% extends 'base.html.twig' %}

{% block title %}CandidateFile{% endblock %}

{% block body %}
    <h1>CandidateFile</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ candidate_file.id }}</td>
            </tr>
            <tr>
                <th>DateCreated</th>
                <td>{{ candidate_file.dateCreated ? candidate_file.dateCreated|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateUpdated</th>
                <td>{{ candidate_file.dateUpdated ? candidate_file.dateUpdated|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>DateDeleted</th>
                <td>{{ candidate_file.dateDeleted ? candidate_file.dateDeleted|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Files</th>
                <td>{{ candidate_file.files }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('candidate_file_index') }}">back to list</a>

    <a href="{{ path('candidate_file_edit', {'id': candidate_file.id}) }}">edit</a>

    {{ include('candidate_file/_delete_form.html.twig') }}
{% endblock %}
